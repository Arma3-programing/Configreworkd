#include <stdio.h>
#include <stdbool.h>
#include <math.h>
//Определение любых констант
#define N 100

//Функция
int main()
{

    //переменныя
    //Переменная short принимает значения ввиде чисел в диапозоне от -32 786 до 32 786
    short a = 21;
    //Переменная unsigned short принимает значения ввиде положительных чисел в диапозоне от 0 до 64 000
    unsigned short b = 50;
    
    //Переменная int  прнимает значения ввиде чисел в диапозоне от -2млн до 2млн(для вывода на экран используеться %d)
    int f = 40;
    // от 0 до 4млн
    unsigned int k = 64;
    
    // Переменная long принимает значения ввиде чисел в диапазоне который в 2 раза больше диапазона переменной int
    long d = 54;
    long long q = 125;
    unsigned long h = 32;
    unsigned long long w = 135;
    
    //Переменная float принимает значения ввиде цифр с точкой(для вывода на экран используеться %f)
    float q1 = 4.5f;
    //Диапазон в 2 раза больше чем у float
    double q2 = 6.5f;
    //Диапазон в 2 раза больше чем у double(для вывода на экран используеться %lf)
    long double q3 = 23.6f;
    
    //Перемення char принимает значение ввиде любых одинарных символов(для вывода на экран используеться %c)
    char q5 = '$';
    
    //Переменная bool принимает значения ввиде true или false. Для использования данной переменной нужно подключить библеотеку stdbool.h
    bool q6 = true;
    
    
    
    int x1, y1, z1, x2, y2, z2, res;
    x1 = 20;
    y1 = 10;
    z1 = 5;
    x2 = -20;
    y2 = -10;
    z2 = -5;
    
    
    res = ( x1 * x1 - 2 * x1 * x2 + x2 * x2) + (y1 * y1 - 2 * y1 * y2 + y2 * y2) + (z1 * z1 - 2 * z1 * z2 + z2 * z2);
    //res = sqrt(res);
    
    printf("Result:%d", res);
    
    
    return 0;
}
//Переменная int  прнимает значения ввиде чисел в диапозоне от -2млн до 2млн(для вывода на экран используеться %d)
  
